
{
  "name": "Plugins",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "plugins",
  "browser_action": {
    "default_icon": {
      "19": "images/icon_19.png",
      "38": "images/icon_38.png"
    },
    "default_title": "Marker"

  },
  "permissions": [
      "http://weibo.com/*",
      "http://m.weibo.cn/*",
      "http://192.168.0.236:3000/*",
      "http://127.0.0.1:3000/*",
      "http://localhost:3000/*"
  ],
  "content_scripts": [{
    "matches": [
      "http://weibo.com/*",
      "http://m.weibo.cn/*",
      "http://192.168.0.236:3000/*",
      "http://127.0.0.1:3000/*",
      "http://localhost:3000/*"
    ],
    "css": ["style.css"],
    "js": ["app.js"]
  }]
}
